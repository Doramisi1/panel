{
    "plugins": [
        "babel-plugin-macros",
        "styled-components",
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "@babel/proposal-optional-chaining",
        "@babel/proposal-nullish-coalescing-operator",
        "@babel/syntax-dynamic-import"
    ],
    "presets": [
        "@babel/typescript",
        ["@babel/react", { "runtime": "automatic" }]
    ]
}
